<h1 class=""><%= @question.title %></h1>
<%= link_to "<< view all questions", questions_path%>
<div class="row">
  <div class="col-md-6">
    <%= @question.body %>
  </div>
  <div class="col-md-6">
    <%= @question.resolved %>
  </div>
  <div class="col-md-4">
    <%= link_to "Edit", edit_question_path(@question), class: "btn btn-success"%>
    <%= link_to "Delete", question_path(@question),method: :delete, class:'btn btn-danger',data: {confirm: "Are you sure you want to delete this question"} %>
  </div>
</div>
